{
    "name": "torrentbits/tracker",
    "description": "Modern BitTorrent Tracker - 2025 Edition",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "firebase/php-jwt": "^6.10",
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^3.5",
        "symfony/http-foundation": "^6.4",
        "symfony/routing": "^6.4",
        "predis/predis": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Controllers\\": "src/Controllers/",
            "App\\Models\\": "src/Models/",
            "App\\Services\\": "src/Services/",
            "App\\Core\\": "src/Core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}


